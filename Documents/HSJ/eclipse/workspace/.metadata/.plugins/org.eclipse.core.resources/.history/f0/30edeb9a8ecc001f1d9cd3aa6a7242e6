package chapter03;

import java.util.Random;

public class R_Random {
	public static void main(String[] args) {
		
		Random random1 = new Random(5);
		Random random2 = new Random(5);
		System.out.println(random1.nextInt()); // 출력:  1791095845
		System.out.println(random2.nextInt()); // 출력:  1791095845
	}
}
